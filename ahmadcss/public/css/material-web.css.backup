    border-radius: var(--md-sys-shape-corner-extra-small) var(--md-sys-shape-corner-extra-small) 0 0;
    padding: var(--md-sys-spacing-3) var(--md-sys-spacing-4);
    font: var(--md-sys-typescale-body-large-font);
    letter-spacing: var(--md-sys-typescale-body-large-tracking);
    color: var(--md-sys-color-on-surface);
    caret-color: var(--md-sys-color-primary);
    transition: all var(--md-sys-motion-duration-short4) var(--md-sys-motion-easing-standard);
}

.theme-material-web .form-control::placeholder {
    color: var(--md-sys-color-on-surface-variant);
    opacity: 0.7;
}

.theme-material-web .form-control:hover {
    background: color-mix(in srgb, var(--md-sys-color-surface-container-highest) 92%, var(--md-sys-color-on-surface));
    border-bottom-color: var(--md-sys-color-on-surface);
}

.theme-material-web .form-control:focus {
    outline: none;
    border-bottom-color: var(--md-sys-color-primary);
    border-bottom-width: 3px;
    background: var(--md-sys-color-surface-container-highest);
}

/* Error State */
.theme-material-web .form-control.is-invalid,
.theme-material-web .form-control[data-error="true"],
.theme-material-web .has-error .form-control {
    border-bottom-color: var(--md-sys-color-error);
