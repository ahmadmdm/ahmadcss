/* === Outlined Text Field === */
.theme-material-web .input-outlined,
.theme-material-web .form-control.outlined {
    background: transparent;
    border: 1px solid var(--md-sys-color-outline);
    border-radius: var(--md-sys-shape-corner-extra-small);
