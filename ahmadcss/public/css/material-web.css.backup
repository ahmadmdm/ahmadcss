/* Remove glass background and gradients */
html.theme-material-web,
html[data-theme="material-web"],
body.theme-material-web,
body[data-theme="material-web"] {
    background: var(--md-sys-color-background) !important;
    background-image: none !important;
    background-attachment: initial !important;
}

body.theme-material-web::before,
body[data-theme="material-web"]::before {
    display: none !important;
    background: none !important;
}

/* Reset font to Material font stack */
body.theme-material-web,
body.theme-material-web *,
body[data-theme="material-web"],
body[data-theme="material-web"] * {
    font-family: var(--md-sys-typescale-font-family-plain) !important;
}

/* Arabic font override */
body.theme-material-web [lang="ar"],
body.theme-material-web :lang(ar),
body[data-theme="material-web"] [lang="ar"],
body[data-theme="material-web"] :lang(ar) {
    font-family: var(--md-sys-typescale-font-family-arabic) !important;
}

/* Reset text colors */
body.theme-material-web,
body.theme-material-web p,
body.theme-material-web span,
body.theme-material-web div,
body[data-theme="material-web"],
body[data-theme="material-web"] p,
body[data-theme="material-web"] span,
body[data-theme="material-web"] div {
    color: var(--md-sys-color-on-surface) !important;
}

/* Reset link colors */
body.theme-material-web a:not(.btn):not(.nav-link):not(.dropdown-item),
body[data-theme="material-web"] a:not(.btn):not(.nav-link):not(.dropdown-item) {
    color: var(--md-sys-color-primary) !important;
}

body.theme-material-web a:not(.btn):not(.nav-link):not(.dropdown-item):hover,
body[data-theme="material-web"] a:not(.btn):not(.nav-link):not(.dropdown-item):hover {
    color: var(--md-sys-color-primary) !important;
    text-decoration: underline;
}

/* â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
   ðŸŒˆ ADDITIONAL BASE STYLES & RESETS
   â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• */

.theme-material-web *,
.theme-material-web *::before,
.theme-material-web *::after {
    box-sizing: border-box;
    transition-property: background-color, border-color, color, box-shadow, transform, opacity;
    transition-duration: var(--md-sys-motion-duration-short3);
    transition-timing-function: var(--md-sys-motion-easing-standard);
}

/* Arabic/RTL Typography */
.theme-material-web *:lang(ar),
.theme-material-web [dir="rtl"],
.theme-material-web [dir="rtl"] * {
    font-family: var(--md-sys-typescale-font-family-arabic);
    letter-spacing: 0;
}

/* Selection */
.theme-material-web ::selection {
    background: var(--md-sys-color-primary-container);
    color: var(--md-sys-color-on-primary-container);
}

/* Scrollbar Styling */
.theme-material-web ::-webkit-scrollbar {
    width: 8px;
    height: 8px;
}

.theme-material-web ::-webkit-scrollbar-track {
    background: var(--md-sys-color-surface-container);
    border-radius: var(--md-sys-shape-corner-full);
}

.theme-material-web ::-webkit-scrollbar-thumb {
    background: var(--md-sys-color-outline-variant);
    border-radius: var(--md-sys-shape-corner-full);
    border: 2px solid var(--md-sys-color-surface-container);
}

.theme-material-web ::-webkit-scrollbar-thumb:hover {
    background: var(--md-sys-color-outline);
}

