}

/* === Labels (Static - NOT Floating) === */
.theme-material-web .control-label,
.theme-material-web .frappe-control > label,
.theme-material-web .form-group > label {
    position: static !important;
    display: block;
    font: var(--md-sys-typescale-label-medium-font);
    letter-spacing: var(--md-sys-typescale-label-medium-tracking);
    color: var(--md-sys-color-on-surface-variant);
    margin-bottom: var(--md-sys-spacing-1);
    text-transform: uppercase;
    font-weight: 500;
    transform: none !important;
}

/* === Filled Text Field === */
.theme-material-web .form-control,
.theme-material-web .input-with-feedback,
