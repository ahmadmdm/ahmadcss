    letter-spacing: var(--md-sys-typescale-body-medium-tracking);
    color: var(--md-sys-color-on-surface-variant);
    overflow-y: auto;
    flex: 1;
}

/* === Dialog Footer/Actions === */
.theme-material-web .modal-footer,
.theme-material-web .dialog-actions {
