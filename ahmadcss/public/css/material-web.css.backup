    --md-sys-color-on-info: #003258;
    --md-sys-color-info-container: #00497D;
    --md-sys-color-on-info-container: #D1E4FF;
    
    /* === Surface Dark === */
    --md-sys-color-surface: #101414;
    --md-sys-color-on-surface: #E1E3E3;
