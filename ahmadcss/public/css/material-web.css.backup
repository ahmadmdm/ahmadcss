/* Ripple Animation */
@keyframes md-ripple {
    0% {
        transform: translate(-50%, -50%) scale(0);
        opacity: 0.24;
    }
    100% {
        transform: translate(-50%, -50%) scale(4);
        opacity: 0;
    }
}

@keyframes md-ripple-unbounded {
    0% {
        transform: translate(-50%, -50%) scale(0);
        opacity: 0.16;
    }
    100% {
        transform: translate(-50%, -50%) scale(2.5);
        opacity: 0;
    }
}

/* Ripple Container */
.theme-material-web .md-ripple,
.theme-material-web [data-ripple] {
    position: relative;
    overflow: hidden;
    -webkit-tap-highlight-color: transparent;
}

/* Ripple Surface */
.theme-material-web .md-ripple .ripple-surface,
.theme-material-web [data-ripple]::after {
    content: '';
    position: absolute;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background: currentColor;
    opacity: 0;
    pointer-events: none;
    transform: translate(-50%, -50%) scale(0);
}

.theme-material-web .md-ripple:active .ripple-surface,
.theme-material-web [data-ripple]:active::after {
    animation: md-ripple var(--md-sys-motion-duration-long2) var(--md-sys-motion-easing-standard) forwards;
}

/* Unbounded Ripple (for icons) */
.theme-material-web .md-ripple-unbounded {
    overflow: visible;
}

.theme-material-web .md-ripple-unbounded::after {
    width: 200%;
    height: 200%;
    animation-name: md-ripple-unbounded;
}

