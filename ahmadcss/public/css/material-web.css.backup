}

/* Dialog Icon */
.theme-material-web .modal-header .dialog-icon,
.theme-material-web .dialog-header .dialog-icon {
    width: 24px;
    height: 24px;
    color: var(--md-sys-color-secondary);
    align-self: center;
}

/* Dialog Title */
.theme-material-web .modal-title,
.theme-material-web .dialog-title {
    font: var(--md-sys-typescale-headline-small-font);
    letter-spacing: var(--md-sys-typescale-headline-small-tracking);
    color: var(--md-sys-color-on-surface);
    margin: 0;
    text-align: center;
}

/* Close Button */
.theme-material-web .modal-header .close,
.theme-material-web .dialog-header .close-btn {
