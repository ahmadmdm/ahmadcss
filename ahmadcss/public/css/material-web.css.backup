/* Linear Progress */
.theme-material-web .progress,
.theme-material-web .progress-bar-container {
    width: 100%;
    height: 4px;
    background: var(--md-sys-color-surface-container-highest);
    border-radius: var(--md-sys-shape-corner-full);
    overflow: hidden;
}

.theme-material-web .progress-bar,
.theme-material-web .progress .bar {
    height: 100%;
    background: var(--md-sys-color-primary);
    border-radius: var(--md-sys-shape-corner-full);
    transition: width var(--md-sys-motion-duration-medium2) var(--md-sys-motion-easing-standard);
}

/* Indeterminate Linear Progress */
.theme-material-web .progress.indeterminate .progress-bar,
.theme-material-web .progress-indeterminate .bar {
    width: 50%;
    animation: linear-progress-indeterminate 2s infinite var(--md-sys-motion-easing-standard);
}

@keyframes linear-progress-indeterminate {
    0% {
        transform: translateX(-100%);
    }
    100% {
        transform: translateX(300%);
    }
}

/* Circular Progress */
.theme-material-web .spinner,
.theme-material-web .circular-progress {
    width: 48px;
    height: 48px;
    animation: circular-rotate 1.4s linear infinite;
}

.theme-material-web .spinner circle,
.theme-material-web .circular-progress circle {
    stroke: var(--md-sys-color-primary);
    stroke-width: 4;
    fill: none;
    stroke-linecap: round;
    animation: circular-dash 1.4s ease-in-out infinite;
}

@keyframes circular-rotate {
    100% {
        transform: rotate(360deg);
    }
}

@keyframes circular-dash {
    0% {
        stroke-dasharray: 1, 200;
        stroke-dashoffset: 0;
    }
    50% {
        stroke-dasharray: 100, 200;
        stroke-dashoffset: -15;
    }
    100% {
        stroke-dasharray: 100, 200;
        stroke-dashoffset: -125;
    }
}

/* â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
