    padding: var(--md-sys-spacing-2) 0;
    min-width: 112px;
    max-width: 280px;
    overflow: hidden;
    animation: menu-enter var(--md-sys-motion-duration-short4) var(--md-sys-motion-easing-emphasized-decelerate);
}

@keyframes menu-enter {
    from {
        opacity: 0;
        transform: scaleY(0.8);
        transform-origin: top;
    }
    to {
        opacity: 1;
        transform: scaleY(1);
    }
}

/* === Menu Item === */
.theme-material-web .dropdown-item,
.theme-material-web .menu-item {
    display: flex;
    align-items: center;
    gap: var(--md-sys-spacing-3);
    min-height: 48px;
    padding: 0 var(--md-sys-spacing-3);
    font: var(--md-sys-typescale-body-large-font);
    letter-spacing: var(--md-sys-typescale-body-large-tracking);
    color: var(--md-sys-color-on-surface);
    cursor: pointer;
    position: relative;
    text-decoration: none;
    border: none;
    background: transparent;
    width: 100%;
    text-align: left;
}

[dir="rtl"] .theme-material-web .dropdown-item,
[dir="rtl"] .theme-material-web .menu-item {
    text-align: right;
}

.theme-material-web .dropdown-item::before,
.theme-material-web .menu-item::before {
    content: '';
    position: absolute;
    inset: 0;
    background: var(--md-sys-color-on-surface);
    opacity: 0;
    transition: opacity var(--md-sys-motion-duration-short3);
    pointer-events: none;
}

.theme-material-web .dropdown-item:hover::before,
.theme-material-web .menu-item:hover::before {
    opacity: var(--md-sys-state-hover-state-layer-opacity);
}

.theme-material-web .dropdown-item:focus-visible::before,
.theme-material-web .menu-item:focus-visible::before {
    opacity: var(--md-sys-state-focus-state-layer-opacity);
}

.theme-material-web .dropdown-item:active::before,
.theme-material-web .menu-item:active::before {
    opacity: var(--md-sys-state-pressed-state-layer-opacity);
}

/* Disabled Menu Item */
.theme-material-web .dropdown-item.disabled,
.theme-material-web .menu-item.disabled {
    color: color-mix(in srgb, var(--md-sys-color-on-surface) 38%, transparent);
    pointer-events: none;
}

/* Selected Menu Item */
.theme-material-web .dropdown-item.active,
.theme-material-web .dropdown-item.selected,
.theme-material-web .menu-item.selected {
    background: color-mix(in srgb, var(--md-sys-color-on-secondary-container) 12%, var(--md-sys-color-secondary-container));
}

/* === Menu Item Icon === */
.theme-material-web .dropdown-item .icon,
.theme-material-web .menu-item .leading-icon {
    width: 24px;
    height: 24px;
    color: var(--md-sys-color-on-surface-variant);
    flex-shrink: 0;
}

/* === Menu Item Trailing === */
.theme-material-web .dropdown-item .trailing,
.theme-material-web .menu-item .trailing {
    margin-left: auto;
    color: var(--md-sys-color-on-surface-variant);
    font: var(--md-sys-typescale-label-large-font);
}

[dir="rtl"] .theme-material-web .dropdown-item .trailing,
[dir="rtl"] .theme-material-web .menu-item .trailing {
    margin-left: 0;
    margin-right: auto;
}

/* === Menu Divider === */
.theme-material-web .dropdown-divider,
.theme-material-web .menu-divider {
    height: 1px;
    background: var(--md-sys-color-outline-variant);
    margin: var(--md-sys-spacing-2) 0;
    border: none;
}

/* === Menu Header === */
.theme-material-web .dropdown-header,
.theme-material-web .menu-header {
    padding: var(--md-sys-spacing-2) var(--md-sys-spacing-3);
    font: var(--md-sys-typescale-label-large-font);
    letter-spacing: var(--md-sys-typescale-label-large-tracking);
    color: var(--md-sys-color-on-surface-variant);
}

/* === Cascading Menu Arrow === */
.theme-material-web .dropdown-item.has-submenu::after,
.theme-material-web .menu-item.has-submenu::after {
    content: 'â€º';
    margin-left: auto;
    font-size: 18px;
    color: var(--md-sys-color-on-surface-variant);
}

[dir="rtl"] .theme-material-web .dropdown-item.has-submenu::after,
[dir="rtl"] .theme-material-web .menu-item.has-submenu::after {
    content: 'â€¹';
    margin-left: 0;
