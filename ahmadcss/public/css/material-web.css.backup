}

.theme-material-web .btn-tonal:hover::before {
    opacity: var(--md-sys-state-hover-state-layer-opacity);
}

/* === Outlined Button === */
.theme-material-web .btn-secondary,
.theme-material-web .btn-default,
.theme-material-web .btn-outline {
    background: transparent;
    color: var(--md-sys-color-primary);
