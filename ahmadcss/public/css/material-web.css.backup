.theme-material-web .tooltip,
.theme-material-web [data-tooltip]::after {
    background: var(--md-sys-color-inverse-surface);
    color: var(--md-sys-color-inverse-on-surface);
    font: var(--md-sys-typescale-body-small-font);
    letter-spacing: var(--md-sys-typescale-body-small-tracking);
    padding: var(--md-sys-spacing-1) var(--md-sys-spacing-2);
    border-radius: var(--md-sys-shape-corner-extra-small);
    max-width: 200px;
    text-align: center;
    white-space: nowrap;
    animation: tooltip-enter var(--md-sys-motion-duration-short4) var(--md-sys-motion-easing-emphasized-decelerate);
}

@keyframes tooltip-enter {
    from {
        opacity: 0;
        transform: scale(0.8);
    }
    to {
        opacity: 1;
        transform: scale(1);
    }
}

/* Rich Tooltip */
.theme-material-web .tooltip-rich {
    background: var(--md-sys-color-surface-container);
    color: var(--md-sys-color-on-surface);
    padding: var(--md-sys-spacing-3);
    border-radius: var(--md-sys-shape-corner-medium);
    box-shadow: var(--md-sys-elevation-level2);
    max-width: 320px;
    white-space: normal;
}

.theme-material-web .tooltip-rich .tooltip-title {
    font: var(--md-sys-typescale-title-small-font);
    margin-bottom: var(--md-sys-spacing-1);
}

.theme-material-web .tooltip-rich .tooltip-text {
    font: var(--md-sys-typescale-body-medium-font);
    color: var(--md-sys-color-on-surface-variant);
}

/* â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
   ðŸ“± RESPONSIVE DESIGN - Adaptive Layouts
   â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• */

/* === Compact (Mobile: 0-599px) === */
@media (max-width: 599px) {
    .theme-material-web .navbar {
        height: 56px;
    }
    
    .theme-material-web .navbar .search-bar {
        display: none;
    }
    
    /* Bottom Navigation Bar */
    .theme-material-web .desk-sidebar,
    .theme-material-web .sidebar {
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
        top: auto;
        width: 100%;
        height: 80px;
        flex-direction: row;
        justify-content: space-around;
        border-right: none;
        border-top: 1px solid var(--md-sys-color-surface-variant);
        box-shadow: var(--md-sys-elevation-level2);
        z-index: 1000;
        padding: var(--md-sys-spacing-2) 0;
        gap: 0;
    }
    
    .theme-material-web .sidebar-menu .desk-sidebar-item,
    .theme-material-web .sidebar .sidebar-item {
        width: auto;
        min-width: 64px;
        height: 64px;
        margin: 0;
    }
    
    .theme-material-web .sidebar-menu .desk-sidebar-item .label,
    .theme-material-web .sidebar .sidebar-item .label {
        font: var(--md-sys-typescale-label-small-font);
    }
    
    /* Page adjustments */
    .theme-material-web .page-container,
    .theme-material-web .layout-main-section {
        padding: var(--md-sys-spacing-3);
        padding-bottom: 96px; /* Space for bottom nav */
    }
    
    .theme-material-web .page-title {
        font: var(--md-sys-typescale-headline-small-font);
    }
    
    /* Form sections */
    .theme-material-web .form-section {
        padding: var(--md-sys-spacing-4);
        margin-bottom: var(--md-sys-spacing-3);
    }
    
    /* Cards */
    .theme-material-web .card-grid {
        grid-template-columns: 1fr;
    }
    
    /* Dialogs become full screen */
    .theme-material-web .modal-content,
    .theme-material-web .dialog {
        max-width: 100%;
        max-height: 100%;
        height: 100vh;
        width: 100vw;
        border-radius: 0;
        margin: 0;
    }
    
    /* FAB positioning */
    .theme-material-web .fab,
    .theme-material-web .btn-fab {
        position: fixed;
        bottom: 96px;
        right: var(--md-sys-spacing-4);
        z-index: 999;
    }
    
    [dir="rtl"] .theme-material-web .fab,
    [dir="rtl"] .theme-material-web .btn-fab {
        right: auto;
        left: var(--md-sys-spacing-4);
    }
    
    /* Tables - Horizontal scroll */
    .theme-material-web .table-responsive {
        overflow-x: auto;
        -webkit-overflow-scrolling: touch;
    }
    
    /* Typography adjustments */
    .theme-material-web .stat-card .stat-number {
        font: var(--md-sys-typescale-headline-large-font);
    }
}

/* === Medium (Tablet: 600-839px) === */
@media (min-width: 600px) and (max-width: 839px) {
    .theme-material-web .desk-sidebar,
    .theme-material-web .sidebar {
        width: 80px;
    }
    
    .theme-material-web .page-container {
        padding: var(--md-sys-spacing-4);
    }
    
    .theme-material-web .card-grid {
        grid-template-columns: repeat(2, 1fr);
    }
    
    .theme-material-web .modal-content,
    .theme-material-web .dialog {
        max-width: 560px;
        max-height: calc(100vh - 96px);
    }
}

/* === Expanded (Desktop: 840-1199px) === */
@media (min-width: 840px) and (max-width: 1199px) {
    .theme-material-web .desk-sidebar,
    .theme-material-web .sidebar {
        width: 80px;
    }
    
    .theme-material-web .card-grid {
        grid-template-columns: repeat(3, 1fr);
    }
}

/* === Large (Large Desktop: 1200px+) === */
@media (min-width: 1200px) {
    .theme-material-web .desk-sidebar.expanded,
    .theme-material-web .sidebar.expanded {
        width: 360px;
    }
    
    .theme-material-web .card-grid {
        grid-template-columns: repeat(4, 1fr);
    }
    
    .theme-material-web .page-container {
        max-width: 1400px;
    }
}

/* === Extra Large (1600px+) === */
@media (min-width: 1600px) {
    .theme-material-web .card-grid {
        grid-template-columns: repeat(5, 1fr);
    }
    
    .theme-material-web .page-container {
        max-width: 1600px;
    }
}

/* === Hover Media Query (for touch devices) === */
@media (hover: none) {
    .theme-material-web .btn-primary:hover,
    .theme-material-web .btn-secondary:hover,
    .theme-material-web .list-row:hover,
    .theme-material-web .dropdown-item:hover {
        /* Disable hover effects on touch devices */
    }
    
    .theme-material-web .btn-primary:hover::before,
    .theme-material-web .btn-secondary:hover::before,
    .theme-material-web .list-row:hover::before,
    .theme-material-web .dropdown-item:hover::before {
        opacity: 0;
    }
}

/* === Reduced Motion === */
@media (prefers-reduced-motion: reduce) {
    .theme-material-web *,
    .theme-material-web *::before,
    .theme-material-web *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
    }
}

/* === Print Styles === */
@media print {
    .theme-material-web .navbar,
    .theme-material-web .desk-sidebar,
    .theme-material-web .sidebar,
    .theme-material-web .fab,
    .theme-material-web .btn-fab {
        display: none !important;
    }
    
    .theme-material-web .page-container {
        max-width: 100%;
        padding: 0;
    }
    
    .theme-material-web .form-section,
    .theme-material-web .card {
        box-shadow: none;
        border: 1px solid #ddd;
        break-inside: avoid;
    }
}

