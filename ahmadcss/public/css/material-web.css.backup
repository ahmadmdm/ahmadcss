/* All text that might be white - force to primary or on-surface */
.theme-material-web .text-white,
.theme-material-web [style*="color: white"],
.theme-material-web [style*="color:#fff"],
.theme-material-web [style*="color: #fff"] {
    color: var(--md-sys-color-primary) !important;
}

/* Tooltip styling */
.theme-material-web .tooltip,
.theme-material-web .popover {
    background: var(--md-sys-color-inverse-surface) !important;
    color: var(--md-sys-color-inverse-on-surface) !important;
    border-radius: var(--md-sys-shape-corner-small);
}

.theme-material-web .tooltip-inner {
    background: var(--md-sys-color-inverse-surface) !important;
    color: var(--md-sys-color-inverse-on-surface) !important;
}

