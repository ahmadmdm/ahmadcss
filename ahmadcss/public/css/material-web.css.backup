   â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• */

/* List View Container */
.theme-material-web .frappe-list,
.theme-material-web [data-page-container] .list-view-container {
    background: var(--md-sys-color-surface);
    border-radius: var(--md-sys-shape-corner-large);
    overflow: hidden;
    border: 1px solid var(--md-sys-color-outline-variant);
}

/* Force Table Layout for consistency */
.theme-material-web .frappe-list .result {
    display: table !important;
    width: 100% !important;
    border-collapse: collapse;
}

/* List Header Row - Table Header */
.theme-material-web .list-row-head,
.theme-material-web .list-headers,
.theme-material-web .result > .list-row:first-child {
    display: table-row !important;
    background: var(--md-sys-color-surface-container-high) !important;
}

.theme-material-web .list-row-head .list-row-col,
.theme-material-web .list-row-head .list-header-subject,
.theme-material-web .list-headers .list-row-col {
    display: table-cell !important;
    padding: 12px 16px !important;
    font-size: 12px !important;
    font-weight: 600 !important;
    text-transform: uppercase !important;
    letter-spacing: 0.5px !important;
    color: var(--md-sys-color-on-surface-variant) !important;
    border-bottom: 2px solid var(--md-sys-color-outline-variant) !important;
    vertical-align: middle !important;
    white-space: nowrap;
}

/* List Data Rows - Table Rows */
.theme-material-web .result .list-row {
    display: table-row !important;
    background: var(--md-sys-color-surface) !important;
    transition: background-color 0.15s ease;
}

.theme-material-web .result .list-row:nth-child(odd) {
    background: var(--md-sys-color-surface) !important;
}

.theme-material-web .result .list-row:nth-child(even) {
    background: var(--md-sys-color-surface-container-lowest) !important;
}

.theme-material-web .result .list-row:hover {
    background: var(--md-sys-color-surface-container) !important;
}

.theme-material-web .result .list-row.selected {
    background: var(--md-sys-color-primary-container) !important;
}

/* List Row Columns - Table Cells */
.theme-material-web .list-row .list-row-col,
.theme-material-web .list-row-col {
    display: table-cell !important;
    padding: 12px 16px !important;
    font-size: 14px !important;
    color: var(--md-sys-color-on-surface) !important;
    border-bottom: 1px solid var(--md-sys-color-surface-variant) !important;
    vertical-align: middle !important;
}

/* Checkbox Column */
.theme-material-web .list-row .list-row-col.list-row-checkbox,
.theme-material-web .list-row-checkbox {
    width: 40px !important;
    min-width: 40px !important;
    max-width: 40px !important;
    text-align: center !important;
}

/* Subject/Title Column - Make it wider */
.theme-material-web .list-row .list-row-col.list-subject,
.theme-material-web .list-row-col.ellipsis {
    min-width: 200px;
    max-width: 300px;
}

/* List Row Links */
.theme-material-web .list-row a,
.theme-material-web .list-row .like-disabled-input a,
.theme-material-web .result .list-row a {
    color: var(--md-sys-color-primary) !important;
    text-decoration: none;
    font-weight: 500;
}

.theme-material-web .list-row a:hover {
    color: var(--md-sys-color-tertiary) !important;
    text-decoration: underline;
}

/* Status Badges/Indicators - Fixed */
.theme-material-web .indicator-pill,
.theme-material-web .badge {
    display: inline-flex !important;
    align-items: center;
    justify-content: center;
    padding: 4px 10px !important;
    border-radius: 100px !important;
    font-size: 11px !important;
    font-weight: 500 !important;
    white-space: nowrap;
    line-height: 1.2;
}

.theme-material-web .indicator-pill.gray,
.theme-material-web .badge-secondary {
    background: var(--md-sys-color-surface-container-high) !important;
    color: var(--md-sys-color-on-surface-variant) !important;
}

.theme-material-web .indicator-pill.blue,
.theme-material-web .badge-info {
    background: var(--md-sys-color-info-container) !important;
    color: var(--md-sys-color-on-info-container) !important;
}

.theme-material-web .indicator-pill.green,
.theme-material-web .badge-success {
    background: var(--md-sys-color-success-container) !important;
    color: var(--md-sys-color-on-success-container) !important;
}

.theme-material-web .indicator-pill.orange,
.theme-material-web .indicator-pill.yellow,
.theme-material-web .badge-warning {
    background: var(--md-sys-color-warning-container) !important;
    color: var(--md-sys-color-on-warning-container) !important;
}

.theme-material-web .indicator-pill.red,
.theme-material-web .badge-danger {
    background: var(--md-sys-color-error-container) !important;
    color: var(--md-sys-color-on-error-container) !important;
}

/* Ellipsis text overflow */
.theme-material-web .list-row-col.ellipsis,
.theme-material-web .ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* List Filters Area */
.theme-material-web .filter-section,
.theme-material-web .list-filters {
    background: var(--md-sys-color-surface-container-low);
    padding: var(--md-sys-spacing-3) var(--md-sys-spacing-4);
    border-bottom: 1px solid var(--md-sys-color-outline-variant);
    display: flex;
    align-items: center;
    gap: var(--md-sys-spacing-3);
    flex-wrap: wrap;
}

.theme-material-web .filter-section input,
.theme-material-web .filter-section select {
    background: var(--md-sys-color-surface) !important;
    border: 1px solid var(--md-sys-color-outline);
    border-radius: var(--md-sys-shape-corner-small);
    padding: var(--md-sys-spacing-2) var(--md-sys-spacing-3);
    color: var(--md-sys-color-on-surface) !important;
}

/* Filter Button */
.theme-material-web .filter-button,
.theme-material-web .btn-filter {
    background: var(--md-sys-color-surface-container-high) !important;
    color: var(--md-sys-color-on-surface) !important;
    border: 1px solid var(--md-sys-color-outline);
    border-radius: var(--md-sys-shape-corner-small);
}

/* List Paging/Count Area */
.theme-material-web .list-paging-area,
.theme-material-web .list-count {
    background: var(--md-sys-color-surface-container-low);
    padding: var(--md-sys-spacing-3) var(--md-sys-spacing-4);
    color: var(--md-sys-color-on-surface-variant) !important;
    font: var(--md-sys-typescale-body-small-font);
}

/* Checkbox in List */
.theme-material-web .list-row-checkbox,
.theme-material-web .list-row input[type="checkbox"] {
    accent-color: var(--md-sys-color-primary);
}

