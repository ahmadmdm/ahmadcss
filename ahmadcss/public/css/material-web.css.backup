}

.theme-material-web .btn-primary:hover {
    box-shadow: var(--md-sys-elevation-level1);
}

.theme-material-web .btn-primary:hover::before {
    opacity: var(--md-sys-state-hover-state-layer-opacity);
}

.theme-material-web .btn-primary:focus-visible {
    outline: none;
    box-shadow: var(--md-sys-elevation-level0);
}

.theme-material-web .btn-primary:focus-visible::before {
    opacity: var(--md-sys-state-focus-state-layer-opacity);
}

.theme-material-web .btn-primary:active::before {
    opacity: var(--md-sys-state-pressed-state-layer-opacity);
}

.theme-material-web .btn-primary:disabled {
    background: color-mix(in srgb, var(--md-sys-color-on-surface) 12%, transparent);
    color: color-mix(in srgb, var(--md-sys-color-on-surface) 38%, transparent);
    box-shadow: none;
    cursor: not-allowed;
}

/* === Filled Tonal Button === */
.theme-material-web .btn-secondary-filled,
.theme-material-web .btn-tonal {
