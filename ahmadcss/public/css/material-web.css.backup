    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: var(--md-sys-shape-corner-full);
    color: var(--md-sys-color-on-surface-variant);
    background: transparent;
    border: none;
    cursor: pointer;
}

[dir="rtl"] .theme-material-web .modal-header .close,
[dir="rtl"] .theme-material-web .dialog-header .close-btn {
    right: auto;
    left: var(--md-sys-spacing-2);
}

.theme-material-web .modal-header .close:hover,
.theme-material-web .dialog-header .close-btn:hover {
    background: color-mix(in srgb, var(--md-sys-color-on-surface-variant) 8%, transparent);
}

/* === Dialog Body === */
.theme-material-web .modal-body,
.theme-material-web .dialog-body {
    padding: 0 var(--md-sys-spacing-6);
    padding-bottom: var(--md-sys-spacing-6);
