/* Page Background */
.theme-material-web .layout-main,
.theme-material-web #page-container {
    background: var(--md-sys-color-surface);
    min-height: 100vh;
}

/* Main Layout Section */
.theme-material-web .layout-main-section {
    background: var(--md-sys-color-surface);
    padding: var(--md-sys-spacing-4);
    max-width: 1600px;
    margin: 0 auto;
}

